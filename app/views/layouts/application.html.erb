  <body>
    <nav class="navbar navbar-default navbar-fixed-top">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <%= link_to '台大南友會', root_path, class: 'navbar-brand' %>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav">
            <li><%= link_to '府城週', week_path %></li>
            <li><%= link_to '南友之夜', night_path %></li>
            <li class="dropdown">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">FB專頁<span class="caret"></span></a>
              <ul class="dropdown-menu" role="menu">
                <li><%= link_to '台大南友之夜', 'https://www.facebook.com/ntutaa' %></li>
                <li class="divider"></li>
              </ul>
            </li>
          </ul>
          <div class="navbar-form navbar-right">
          <% if session[:user].blank? %>
            <%= link_to '登入', user_login_path, class: 'btn btn-success' %>
            <%= link_to '建立帳號', user_new_path, class: 'btn btn-success' %>
          <% else %>
            <span class="btn btn-success">Hi, <%= session[:user]["name"] %></span>
            <%= link_to '登出', user_logout_path, class: 'btn btn-success' %>
          <% end %>
          </div>
        </div>
      </div>
    </nav>
    <%= yield %>
  </body>
